{
  "name": "minecraft-manager",
  "version": "2.3.0",
  "license": "GPL-3.0",
  "main": "main.js",
  "author": "theemeraldtree",
  "description": "Minecraft Manager",
  "homepage": "https://github.com/theemeraldtree/minecraft-manager",
  "repository": "github:theemeraldtree/minecraft-manager",
  "productName": "Minecraft Manager",
  "build": {
    "appId": "net.theemeraldtree.minecraftmanager",
    "extraResources": [
      "resources/*"
    ],
    "fileAssociations": [
      {
        "ext": "mcjprofile",
        "name": "Minecraft Java Profile (OMAF)",
        "description": "Minecraft Java Profile for OMAF"
      }
    ],
    "win": {
      "publish": {
        "provider": "generic",
        "url": "https://theemeraldtree.net/updates/win"
      }
    },
    "mac": {
      "publish": {
        "provider": "generic",
        "url": "https://theemeraldtree.net/updates/mac"
      }
    },
    "nsis": {
      "license": "LICENSE",
      "perMachine": "true",
      "oneClick": "false"
    }
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "adm-zip": "^0.4.13",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "^4.4.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.0.0",
    "electron-updater": "^4.0.6",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "file-loader": "^3.0.1",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-plugin": "^3.2.0",
    "jimp": "^0.6.4",
    "js-yaml": "^3.13.1",
    "mkdirp": "^0.5.1",
    "prettier": "^1.19.1",
    "prettier-webpack-plugin": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.0",
    "react-click-away-listener": "^0.4.3",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.10.0",
    "react-is": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "rimraf": "^2.6.3",
    "sanitize-html": "^1.20.0",
    "semver": "^6.1.1",
    "styled-components": "^5.0.0-beta.9",
    "styled-transition-group": "^2.0.0",
    "stylis": "^3.5.4",
    "typeface-roboto": "^0.0.75",
    "webpack": "4.29.0",
    "webpack-dev-server": "3.1.14",
    "electron": "^4.0.1"
  },
  "devDependencies": {
    "electron-builder": "^20.38.5",
    "webpack-cli": "3.2.1"
  },
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --port 9483",
    "compile": "webpack",
    "build": "electron-builder ./",
    "package": "npm run compile && npm run build",
    "lint": "npx eslint src",
    "lint-fix": "npx eslint src --fix"
  }
}
